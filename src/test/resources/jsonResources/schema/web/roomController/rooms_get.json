{
  "$id": "http://example.com/example.json",
  "type": "object",
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "name": {
      "$id": "/properties/name",
      "type": "string",
      "title": "The Name Schema ",
      "default": "",
      "examples": [
        "巴萨卡楼宇测试"
      ]
    },
    "rooms": {
      "$id": "/properties/rooms",
      "type": "array",
      "items": {
        "$id": "/properties/rooms/items",
        "type": "object",
        "properties": {
          "priceUnit": {
            "$id": "/properties/rooms/items/properties/priceUnit",
            "type": "string",
            "title": "The Priceunit Schema ",
            "default": "",
            "examples": [
              "D"
            ]
          },
          "roomNumber": {
            "$id": "/properties/rooms/items/properties/roomNumber",
            "type": "string",
            "title": "The Roomnumber Schema ",
            "default": "",
            "examples": [
              "102"
            ]
          },
          "id": {
            "$id": "/properties/rooms/items/properties/id",
            "type": "integer",
            "title": "The Id Schema ",
            "default": 0,
            "examples": [
              170634
            ]
          },
          "floor": {
            "$id": "/properties/rooms/items/properties/floor",
            "type": "string",
            "title": "The Floor Schema ",
            "default": "",
            "examples": [
              "2"
            ]
          },
          "areaSize": {
            "$id": "/properties/rooms/items/properties/areaSize",
            "type": "integer",
            "title": "The Areasize Schema ",
            "default": 0,
            "examples": [
              1200
            ]
          }
        }
      }
    },
    "roomAreaSize": {
      "$id": "/properties/roomAreaSize",
      "type": "integer",
      "title": "The Roomareasize Schema ",
      "default": 0,
      "examples": [
        1300
      ]
    },
    "id": {
      "$id": "/properties/id",
      "type": "integer",
      "title": "The Id Schema ",
      "default": 0,
      "examples": [
        3352
      ]
    },
    "imageUrl": {
      "$id": "/properties/imageUrl",
      "type": "string",
      "title": "The Imageurl Schema ",
      "default": "",
      "examples": [
        "/2018/01/dae2l8ic8yjqiocwa5hkxww86amf22md.jpg"
      ]
    }
  }
}